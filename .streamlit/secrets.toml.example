# Example Streamlit secrets configuration
# Copy this file to .streamlit/secrets.toml and add your actual API keys

[api_keys]
# Get your Finnhub API key from: https://finnhub.io/
finnhub = "your_finnhub_api_key_here"

# Get your MarketAux API key from: https://marketaux.com/
MARKETAUX_API_KEY = "your_marketaux_api_key_here"

# Example format:
# finnhub = "c123456789abcdef"
# MARKETAUX_API_KEY = "v9876543210zyxwvu"

